{
  "name": "your-memories",
  "version": "0.0.1",
  "description": "A view into your memories.",
  "author": "Nicolas Inden",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:clean && faucet --compact && tsc --project ./tsconfig.compile.json",
    "build:clean": "rm -rf ./build",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "npm run barrels && faucet --watch & tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "./utils/db_migration.sh && cross-env NODE_ENV=production node dist/index.js",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test:unit",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "migrate": "env NODE_ENV=production node-pg-migrate up",
    "migrate:dev": "env NODE_ENV=development node-pg-migrate up",
    "migrate:test": "env NODE_ENV=test node-pg-migrate up",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@prisma/client": "^3.14.0",
    "@tsed/ajv": "^6.114.16",
    "@tsed/common": "^6.114.16",
    "@tsed/core": "^6.114.16",
    "@tsed/di": "^6.114.16",
    "@tsed/engines": "^6.114.16",
    "@tsed/exceptions": "^6.114.16",
    "@tsed/json-mapper": "^6.114.16",
    "@tsed/logger": "^6.1.4",
    "@tsed/logger-file": "^6.1.4",
    "@tsed/platform-cache": "^6.114.16",
    "@tsed/platform-exceptions": "^6.114.16",
    "@tsed/platform-express": "^6.114.16",
    "@tsed/platform-log-middleware": "^6.114.16",
    "@tsed/platform-middlewares": "^6.114.16",
    "@tsed/platform-params": "^6.114.16",
    "@tsed/platform-response-filter": "^6.114.16",
    "@tsed/platform-views": "^6.114.16",
    "@tsed/prisma": "^6.114.16",
    "@tsed/schema": "^6.114.16",
    "ajv": "^8.11.0",
    "barrelsby": "^2.3.4",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.1.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "method-override": "^3.0.0",
    "node-pg-migrate": "^6.2.1",
    "nunjucks": "^3.2.3",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.20.17",
    "@tsed/cli-plugin-jest": "3.20.17",
    "@tsed/cli-plugin-prisma": "3.20.17",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.40",
    "@types/nunjucks": "^3.2.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faucet-pipeline-js": "^2.1.8",
    "faucet-pipeline-sass": "^1.7.1",
    "faucet-pipeline-static": "^2.0.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "default",
    "architecture": "default"
  }
}
